<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">

    <em:id>cloudfile-webdav@mozilla.kewis.ch</em:id>
    <em:version>1.0</em:version>
    <em:type>2</em:type>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <!-- Thunderbird -->
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>14.0</em:minVersion>
        <em:maxVersion>16.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>WebDAV for Filelink</em:name>
    <em:description>Adds the possibility to use a WebDAV store for Filelink</em:description>
    <em:creator>Philipp Kewisch</em:creator>
    <em:homepageURL>http://mozilla.kewis.ch/</em:homepageURL>
    <em:contributor>Mark James (http://www.famfamfam.com/lab/icons/silk/)</em:contributor>
    <em:iconURL>chrome://cloudfile-webdav/skin/webdav_32.png</em:iconURL>

    <em:localized>
      <Description>
        <em:locale>de</em:locale>
        <em:name>WebDAV für Filelink</em:name>
        <em:description>Ermöglicht es, ein WebDAV Dienst für Filelink zu verwenden</em:description>
        <em:translator>Philipp Kewisch</em:translator>
      </Description>
    </em:localized>
  </Description>
</RDF>
